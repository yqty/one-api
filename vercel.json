{
  "version": 2,
  "builds": [
    {
      "src": "api/main.go",
      "use": "@vercel/go"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api"
    }
  ],
  "env": {
    "GIN_MODE": "release",
    "SQL_DSN": "your-database-connection-string",
    "REDIS_CONN_STRING": "your-redis-connection-string",
    "SESSION_SECRET": "your-session-secret"
  }
} 
